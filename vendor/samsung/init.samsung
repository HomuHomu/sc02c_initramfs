#!/sbin/busybox sh
export _PATH="$PATH"
export PATH=/sbin

if [ "$1" = 'secondary' ]; then
  busybox cp -a /vendor/samsung/init.rc.1 /init.rc
else
  busybox cp -a /vendor/samsung/init.rc.0 /init.rc
fi

busybox cp -a /vendor/samsung/init.smdk4210.rc /
busybox cp -a /vendor/samsung/init.smdk4210.usb.rc /
busybox cp -a /vendor/samsung/ueventd.rc /
busybox cp -a /vendor/samsung/ueventd.smdk4210.rc /

mv /_init /init
exec /init
